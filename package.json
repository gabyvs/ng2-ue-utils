{
  "name": "ng2-ue-utils",
  "version": "1.1.0",
  "description": "Set of angular2 components, directives, pipes and services that will be shared among unified experience SPAs",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "postinstall": "npm run typingsInstall",
    "start:neza": "node node_modules/nezaldi --conf=devserver-config.json",
    "start:webpack": "node ./webpack/webpack.server",
    "start": "npm run start:webpack & npm run start:neza",
    "test:phantom": "karma start karma.conf.js",
    "test:debug": "karma start karma.debug.js --no-single-run --auto-watch --browsers Chrome",
    "test": "npm run test:phantom",
    "typingsInstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabyvs/ng2-ue-utils.git"
  },
  "keywords": [
    "angular2",
    "alm"
  ],
  "author": "Gabriela Vazquez",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gabyvs/ng2-ue-utils/issues"
  },
  "homepage": "https://github.com/gabyvs/ng2-ue-utils#readme",
